<script>
let scrollInterval;
let isScrolling = false;

document.getElementById('scroll-left').addEventListener('mousedown', function() {
    startScrolling(-200);
});
document.getElementById('scroll-right').addEventListener('mousedown', function() {
    startScrolling(200);
});

document.addEventListener('mouseup', stopScrolling);
document.getElementById('scroll-left').addEventListener('mouseleave', stopScrolling);
document.getElementById('scroll-right').addEventListener('mouseleave', stopScrolling);

// Add touch event listeners
document.getElementById('scroll-left').addEventListener('touchstart', function(e) {
    e.preventDefault();
    startScrolling(-200);
});
document.getElementById('scroll-right').addEventListener('touchstart', function(e) {
    e.preventDefault();
    startScrolling(200);
});

document.addEventListener('touchend', stopScrolling);
document.getElementById('scroll-left').addEventListener('touchend', stopScrolling);
document.getElementById('scroll-right').addEventListener('touchend', stopScrolling);

document.getElementById('tab-container').addEventListener('scroll', checkScroll);

function startScrolling(direction) {
    if (scrollInterval) return;
    isScrolling = true;
    scrollInterval = setInterval(function() {
        var container = document.getElementById('tab-container');
        container.scrollBy({
            left: direction,
            behavior: 'smooth'
        });
        checkScroll();
    }, 100); // Adjust the interval time as needed
}

function stopScrolling() {
    clearInterval(scrollInterval);
    scrollInterval = null;
    setTimeout(() => isScrolling = false, 100); // Add a delay to prevent immediate clicks after scrolling
}

function checkScroll() {
    var container = document.getElementById('tab-container');
    var scrollLeft = container.scrollLeft;
    var maxScrollLeft = container.scrollWidth - container.clientWidth;

    if (scrollLeft <= 0) {
        document.getElementById('scroll-left').classList.add('hidden');
    } else {
        document.getElementById('scroll-left').classList.remove('hidden');
    }

    if (scrollLeft >= maxScrollLeft) {
        document.getElementById('scroll-right').classList.add('hidden');
    } else {
        document.getElementById('scroll-right').classList.remove('hidden');
    }
}

checkScroll();
window.addEventListener('resize', checkScroll);


</script>

    <script>
       // Function to show the tab content
    function showTab(tabId) {
        document.querySelectorAll('.post-tab-content').forEach(content => {
            content.style.display = 'none';
        });
        document.getElementById(tabId).style.display = 'block';
        localStorage.setItem('activeTab', tabId);
    }

    document.querySelectorAll('.post-tab').forEach(tab => {
        tab.addEventListener('click', function() {
            document.querySelectorAll('.post-tab').forEach(t => t.classList.remove('active'));
            tab.classList.add('active');
            showTab(tab.getAttribute('onclick').match(/'([^']+)'/)[1]);
        });

        // Add touch event for tabs
        tab.addEventListener('touchstart', function() {
            document.querySelectorAll('.post-tab').forEach(t => t.classList.remove('active'));
            tab.classList.add('active');
            showTab(tab.getAttribute('onclick').match(/'([^']+)'/)[1]);
        });
    });

    // Load the active tab from localStorage
    window.onload = function() {
        const activeTab = localStorage.getItem('activeTab') || 'post-content1';
        showTab(activeTab);
        document.querySelectorAll('.post-tab').forEach(tab => {
            if (tab.getAttribute('onclick').includes(activeTab)) {
                tab.classList.add('active');
            } else {
                tab.classList.remove('active');
            }
        });
    };

    </script>


    <script>
// Menggunakan event listener untuk toggle konten saat head di-klik
document.addEventListener("DOMContentLoaded", function() {
    var faqHeads = document.querySelectorAll('.post-blog-faq-head');
    
    faqHeads.forEach(function(faqHead) {
        faqHead.addEventListener('click', function() {
            var faqItem = this.parentElement;
            var faqContent = faqItem.querySelector('.post-blog-faq-content');
            
            // Toggle display dengan animasi
            if (faqContent.style.display === 'block') {
                faqContent.style.display = 'none';
                faqHead.classList.remove('active');
                faqContent.style.animation = 'postSlideUp 0.3s forwards';
            } else {
                faqContent.style.display = 'block';
                faqHead.classList.add('active');
                faqContent.style.animation = 'postSlideDown 0.3s forwards';
            }
        });
    });
});
</script>
